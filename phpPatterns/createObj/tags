NastyBoss2	NastyBoss2.class.php	/^class NastyBoss2 $/;"	c
addEmployee	NastyBoss2.class.php	/^	\/*{{{*public function addEmployee()*\/$/;"	f
addEmployee	NastyBoss2.class.php	/^	public function addEmployee(Employee $employee) $/;"	f
boss	NastyBoss2.class.php	/^$boss = new NastyBoss2();$/;"	v
employee	NastyBoss2.class.php	/^			$employee = array_pop($employees);$/;"	v
employees	NastyBoss2.class.php	/^	private $employees = array();$/;"	v
projectFails	NastyBoss2.class.php	/^	public function projectFails()$/;"	f
